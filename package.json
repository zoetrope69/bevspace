{
  "private": true,
  "name": "bevspace",
  "version": "0.0.1",
  "description": "final year project",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --progress",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdir -p build && ncp src/assets build/assets",
    "test": "snyk test && npm run lint",
    "lint": "eslint src tests/**/*.js",
    "deploy": "npm run snyk-protect && npm run test && npm run build && surge ./build -d bev.space",
    "snyk-protect": "snyk protect"
  },
  "license": "MIT",
  "author": "Zac Colley <hi@zaccolley.com>",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-minimal": "^1.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "core-js": "^2.1.0",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "json-loader": "^0.5.4",
    "ncp": "^2.0.0",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.5.1",
    "offline-plugin": "^2.0.4",
    "postcss-loader": "^0.8.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "superstatic": "^4.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "brauhaus": "^1.1.1",
    "cradle": "^0.7.1",
    "decko": "^1.1.3",
    "express": "^4.13.4",
    "preact": "^4.1.1",
    "preact-compat": "^1.6.0",
    "preact-router": "^1.2.3",
    "proptypes": "^0.14.3",
    "snyk": "^1.11.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
