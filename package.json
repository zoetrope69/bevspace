{
  "private": true,
  "name": "bevspace",
  "version": "0.0.2",
  "description": "final year project",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "superstatic build -p ${PORT:-8080} --host 0.0.0.0 --gzip -c '{\"rewrites\": [{\"source\":\"**\",\"destination\":\"index.html\"}],\"headers\":[{\"source\":\"**\",\"headers\":[{\"key\":\"Cache-Control\",\"value\":\"max-age=86400\"}]}]}'",
    "prestart": "npm run build",
    "build": "NODE_ENV=production webpack -p --progress && ncp build/index.html build/200.html",
    "prebuild": "rm build -r && mkdir -p build && ncp src/service-worker.js build/service-worker.js && ncp src/manifest.json build/manifest.json && ncp src/assets build/assets",
    "test": "snyk test && npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint {src,test}",
    "deploy": "npm run build && surge --project ./build/ --domain bevspace.surge.sh",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "license": "MIT",
  "author": "Zac Colley <hi@zaccolley.com>",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-minimal": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "chai": "^3.5.0",
    "core-js": "^2.2.2",
    "css-loader": "^0.23.1",
    "cz-conventional-changelog": "^1.1.5",
    "es6-promise": "^3.1.2",
    "eslint": "^2.7.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-es6-shim": "^0.2.3",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "moment": "^2.12.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.6",
    "postcss-loader": "^0.8.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "snyk": "^1.14.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "superstatic": "^4.0.2",
    "surge": "^0.17.7",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "whatwg-fetch": "^0.11.0"
  },
  "dependencies": {
    "brauhaus": "^1.1.1",
    "decko": "^1.1.3",
    "pouchdb": "^5.3.1",
    "pouchdb-authentication": "^0.4.3",
    "preact": "^4.5.1",
    "preact-compat": "^1.6.1",
    "preact-router": "^1.3.0",
    "proptypes": "^0.14.3",
    "wildemitter": "^1.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "snyk": true
}
